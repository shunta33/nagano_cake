  <h2>商品一覧</h2>
  <%= link_to "商品新規登録", new_admin_item_path %>
  <table class="table">
    <thead>
      <tr>
        <th>商品ID</th>
        <th>商品名</th>
        <th>税抜価格</th>
      </tr>
    </thead>
    <tbody>
    <% @items.each do |item| %>
    <tr>
      <td><%= item.id %></td>
      <td><%= link_to item.name, edit_admin_item_path(item.id) %></td>
      <td><%= item.price %></td>
    </tr>
    <% end %>
  </tbody>
  </table>
