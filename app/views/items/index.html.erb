<h1>商品一覧(全x件)</h1>
  <table class="table">
    <thead>
      <tr>
        <th></th>
        <th>商品名</th>
        <th>税抜価格</th>
      </tr>
    </thead>
    <tbody>
    <% @items.each do |item| %>
    <tr>
      <td><%= attachment_image_tag item, :image %></td>
      <td><%= link_to item.name, item_path(item.id) %></td>
      <td><%= item.price %></td>
    </tr>
    <% end %>
  </tbody>
  </table>